{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "graphql-tag": "^2.10.1",
    "history": "^4.7.2",
    "react": "^16.7.0",
    "react-apollo": "^2.4.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.7.0",
    "react-loading": "^2.0.3",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "typescript": "3.2.4",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clear": "find . -name \"__generated__\" -exec rm -rf '{}' +",
    "apollo-codegen": "apollo client:codegen --addTypename --target=typescript --localSchemaFile=../server/src/schema/__generated__/schema.graphql --includes=\"./src/**/*.{ts,tsx}\" --tagName gql",
    "watch": "npm run clear && concurrently --kill-others-on-fail --kill-others npm:watch-*",
    "watch-app": "npm run start",
    "watch-apollo-codegen": "npm run apollo-codegen -- --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/graphql": "^14.0.5",
    "@types/history": "^4.7.2",
    "@types/qs": "^6.5.1",
    "@types/react-bootstrap": "^0.32.15",
    "@types/react-router": "^4.4.3",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^4.3.1",
    "concurrently": "^4.1.0",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2"
  }
}
